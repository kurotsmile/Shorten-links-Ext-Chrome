{
  "manifest_version": 2,
  "name": "Shorten links",
  "version": "1.0",
  "description": "Shortening link application, helping you to have short and beautiful links easily shared with everyone",
  "icons": {
    "128": "images/icon128.png",
    "48": "images/icon48.png",
    "16": "images/icon16.png"
  },
  "browser_action": {
    "default_icon": "images/icon16.png",
    "default_popup": "popup.html"
  },
  "permissions": ["storage","https://carrotstore.com/"]
}